<h2 class="heading">External Works</h2>

<!--navigation-->
<% if admin_signed_in? %>
<p class="navigation actions"><%= link_to "Show duplicates (#{ExternalWork.duplicate.count})", url_for(:show => 'duplicates') %></p>
<% end %>
 <%= will_paginate @external_works %>

<% unless @external_works.blank? %>
  <h3 class="landmark heading">Listing External Works</h3>
	<ol class="work index group">
  <% for external_work in @external_works %>
  	<%= render :partial => 'external_works/blurb', :locals => {:external_work => external_work} %>
  <% end %>
  </ol>
  <%= will_paginate @external_works %>
<% end %>
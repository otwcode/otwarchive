<% # load and cache site skin %>
<% set_site_skin %>
<% if @site_skin %>
  <% skin_cache_key, roles = get_skin_cache(@site_skin) %>
  <% cache('/v1/' + skin_cache_key, skip_digest: true) do %>
    <%= @site_skin.get_style(roles) %>
  <% end %>
<% end %>

<!--sandbox for developers	-->
<%= stylesheet_link_tag 'sandbox', skip_pipeline: true %>
<%= javascript_include_tag 'livevalidation_standalone', skip_pipeline: true %>

<% if ArchiveConfig.USE_ESI %>
  <esi:include src="/csrf_meta_tags.html" />
<% else %>
  <%= csrf_meta_tag %>
<% end %>

<!--approved children-->

<div id="children" class="children module">
  <h3 class="heading"><%= ts('Works inspired by this one:') %></h3>
  <ul>
    <% for child_work in inspired_by %>
      <li>
        <%= link_to child_work.work.title.html_safe, child_work.work %> <%= ts("by") %> <%= byline(child_work.work) %>.
      </li>
    <% end %>
  </ul>
</div>
<!--BACK END: if this is only accessed via the user dashboard it needs the user dashboard view-->
<h2 class="heading"><%= ts('Edit Archive Skin')  %></h2>

<%= error_messages_for :skin %>

<!--subnav-->
<%= render :partial => "skin_top_navigation" %>
<!--/subnav-->

<h3 class="landmark heading"><%= ts("Edit Archive Skin Form") %></h3>
<%= nested_form_for(@skin, :html => {:multipart => true}) do |f| %>

  <% if params[:wizard] %>
    <%= render 'wizard_form', :f => f %>
  <% else %>
    <%= render 'form', :f => f %>
  <% end %>

  <p class="submit actions"><%= f.submit ts('Update')  %></p>

<% end %>
